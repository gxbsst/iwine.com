<div id="login">
  <div class="password">
    <div class="logreg_box">
      <div class="top2"></div>
      <div class="mid">
        <div class="content">
          <h2>重发激活邮件</h2>
          <div class="clearfix">
            <div class="un float_l">
              <%= form_for(resource, :as => resource_name, :url => confirmation_path(resource_name), :html => { :method => :post }) do |f| %>
                <%= f.email_field :email, :placeholder => "请输入邮箱地址", :class => "text" %>
              <% end %>
            </div>
            <%= link_to "javascript:void(0);", :class => "float_l ml10 save" do %>
              <%= image_tag "/assets/waterfall/images/common/btn_submit.jpg", :alt => "登录", :width => "96", :height => "41" %>
            <% end %>
          </div>
          <div class="clear"></div>
        </div>
      </div>
      <div class="bot"></div>
    </div>
  </div>
</div>
<script type="text/javascript">
    $(".save").click(function(){
        $(".new_user").submit();
    });
</script>