<% content_for :css do %>
    <%= theme_stylesheet_link_tag :hp, :add %>
<% end %>
<%= theme_javascript_include_tag 'wine_registers_variety', 'region_world' %>

<div class="whitespace">
  <div class="addwine">
    <%= render "wine_details/addwine_step", :step => "step_sec" %>
    <div class="notice_b mb20">请填写<%= @register.origin_name.blank? ? "该酒款的" : @register.origin_name %>基本信息的基本信息</div>
    <div id="stylized" class="myform">

      <%= render "new_form", :objet => @register %>
    </div>
  </div>
</div>
<script type="text/javascript" charset="utf-8">
    $('#submit_register').click(function(){
        $("form.form-horizontal").submit();
        return false;
    });
</script>

