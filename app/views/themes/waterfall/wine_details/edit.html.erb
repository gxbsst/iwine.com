<% content_for :css do %>
    <%= theme_stylesheet_link_tag :hp, :add %>
<% end %>
<%= javascript_include_tag 'wine_registers_variety', 'region_world' %>
<div class="whitespace">
  <div class="addwine">
    <%= render "wine_details/addwine_step", :step => "step_thr" %>
    <div class="notice_b mb20">请完善<%= @register.name_en.blank? ? "该酒款的" : @register.ename %>的详细信息</div>
    <div id="stylized" class="myform">
    <%=  render "edit_form", :object => @register %>
    </div>
  </div>
</div>
<script type="text/javascript" charset="utf-8">
    $('a.complete_register').click(function(){
        $("form.form-horizontal").submit();
        return false;
    });
</script>
