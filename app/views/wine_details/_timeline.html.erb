<% @timelines.each do |l| %>
<div class="ks-waterfall item about_with_wine">
   <p class="wine_cover">
     <%= wine_waterfall_image_tag(l.secondary_actor, :thumb_name => :w_middle) %>
   </p>
   <h3>
     <%= link_to  wine_path(l.secondary_actor) do %>
       <%= raw l.secondary_actor.cname %><br />
       <span class="font_14"><%= raw l.secondary_actor.ename %></span>
     <% end %>
   </h3>
   <p class="event_type">
     <%= APP_DATA["timeline"]["event"]["user"][l.event_type] %>
   </p>
   <div class="sub_item">
     <div class="pic">
       <%= link_to_user l.actor, user_path(l.actor, :trailing_slash => true), :with_avatar => true, :avatar_version => :thumb %>
     </div>
     <p><%= link_to_user l.actor, user_path(l.actor, :trailing_slash => true) %> <%= l.created_at.to_s(:normal) %> </p>
     <p class="body"><%#= l.subject.body %></p>
   </div>
 </div>
<% end %>