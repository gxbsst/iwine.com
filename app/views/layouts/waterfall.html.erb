<!DOCTYPE html>
<head>
  <title><%= content_for?(:title) ? content_for(:title) : 'Patrick' %></title>
  <%= stylesheet_link_tag    "v2/application" %>
  <%= raw stylesheet_if_exists "#{params[:controller]}", "#{params[:controller]}/#{params[:action]}" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <!--[if lte IE 7]>
  <%= stylesheet_link_tag "v2/ie" %>
  <![endif]-->
  <!--[if lt IE 9]>
  <%= javascript_include_tag "html5" %>
  <![endif]-->
</head>
<body >

  <%= render "share/header" %>
    <div class="main_top"> </div>

  <div id="main">
    <%= yield %>
    <div class="clear"></div>
    <!-- main end -->
  </div>
    <div class="main_bottom"></div>

  <%= render "share/footer" %>

</body>
</html>
